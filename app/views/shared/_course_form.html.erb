<%= simple_form_for @course do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <%= f.input :content, :label => "Course Title" %>
  <%= f.input :body, :label => "Course Description" %>
  <%= f.input :location, :label => "Zip Code", :hint => "This is the zip code of where the course will be held" %>
  <%= f.button :submit %>
<% end %>
  